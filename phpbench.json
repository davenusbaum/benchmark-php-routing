{
	"runner.bootstrap": "config.php",
	"runner.path": "benchmark",
	"runner.file_pattern": "*_Benchmark.php",
	"report.generators":
	{
		"short":
		{
			"extends": "aggregate",
			"cols": [
				"benchmark", "subject", "set",
				"best", "mean", "mode", "worst",
				"stdev", "rstdev",
				"diff"
			],
			"expressions":
			{
				"diff": "percent_diff(mode(suite[\"result_time_avg\"]), mode(result_time_avg))"
			}
		}
	}
}
